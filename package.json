{
  "name": "html-starter",
  "version": "1.0.0",
  "description": "Basic (minimal) boilerplate for HTML, Javascript and SCSS with WebPack, Babel and ESLint using latest versions",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --mode production --config webpack.prod.babel.js",
    "dev": "webpack-dev-server --mode development --config webpack.dev.babel.js --open",
    "lint": "eslint . && sass-lint -v -q"
  },
  "author": "desero",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "babel-minify-webpack-plugin": "^0.3.0",
    "breakpoint-sass": "^2.7.1",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.4",
    "node-sass": "^4.7.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.1",
    "postcss-pxtorem": "^4.0.1",
    "postcss-scss": "^1.0.4",
    "prettier": "1.11.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.2"
  }
}
